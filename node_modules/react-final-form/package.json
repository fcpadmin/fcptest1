      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      ".*\\.ts"
    ]
  },
  "bundlesize": [
    {
      "path": "dist/react-final-form.umd.min.js",
      "threshold": "2kB"
    },
    {
      "path": "dist/react-final-form.es.js",
      "threshold": "3kB"
    },
    {
      "path": "dist/react-final-form.cjs.js",
      "threshold": "3kB"
