    "eslint-plugin-react": "^7.12.4",
    "fast-deep-equal": "^2.0.1",
    "final-form": "^4.12.0",
    "flow-bin": "^0.94.0",
    "glow": "^1.2.2",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.5",
    "nps": "^5.9.4",
    "nps-utils": "^1.7.0",
    "opencollective": "^1.0.3",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
