    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "bundlesize": "^0.17.1",
    "doctoc": "^1.4.0",
    "dtslint": "^0.5.3",
    "eslint": "^5.13.0",
    "eslint-config-react-app": "^3.0.6",
